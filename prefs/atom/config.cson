"*":
  core:
    disabledPackages: [
      "background-tips"
      "metrics"
      "exception-reporting"
    ]
    telemetryConsent: "no"
  editor:
    autoIndentOnPaste: false
    fontFamily: "Source Code Pro for Powerline"
    invisibles: {}
    showInvisibles: true
  "file-icons": {}
  "git-plus":
    general:
      analytics: false
  linter: {}
  "linter-eslint": {}
  "linter-stylelint":
    disableWhenNoConfig: false
    useStandard: true
  "linter-ui-default":
    panelHeight: 69
  pigments:
    mergeColorDuplicates: true
    sortPaletteColors: "by color"
  "prettier-atom":
    formatOnSaveOptions:
      cssScopes: [
        ""
      ]
      enabled: true
      showInStatusBar: true
    prettierOptions:
      singleQuote: true
      trailingComma: "all"
  "tree-view":
    hideVcsIgnoredFiles: false
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
