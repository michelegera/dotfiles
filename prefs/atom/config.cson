"*":
  core:
    disabledPackages: [
      "background-tips"
      "metrics"
      "exception-reporting"
      "hashy"
    ]
  editor:
    autoIndentOnPaste: false
    fontFamily: "Input Mono"
    invisibles: {}
    lineHeight: 1.75
    preferredLineLength: 140
  "file-icons":
    coloured: false
  pigments:
    markerType: "dot"
    mergeColorDuplicates: true
    sortPaletteColors: "by color"
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
